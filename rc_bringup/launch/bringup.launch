<launch>

  <arg name="map_file" default="$(find rc_bringup)/map/hallway.yaml"/>

	<!--gyro -->
	<!--node name="mpu6050_node" pkg="mpu6050" type="mpu6050_node"/-->

	<!--remote controll -->	
	<node pkg="rc_bringup" name="rc_node" type="rc_control.py" output="screen"/>
  
	<!-- Rplidar -->
	<include file="$(find rc_bringup)/launch/rplidar.launch"/>

	<!-- Navigation -->
	<include file="$(find rc_bringup)/launch/navigation.launch"/>

	<!-- Map server -->
	<node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)" />

</launch>
